#arithmetic operators
a = 5
b = 2

print(a + b)
print(a - b)
print(a * b)
print(a / b)#always float value.
print(a ** b)#a^b
print(a % b)#remainder

#relational operator
a=50
b=20

print(a==b)#False
print(a!=b)#True
print(a>=b)#True
print(a>b)#True
print(a<=b)#False
print(a<b)#False

#assignment operators
#num = 10
# num = num + 10
num = 10
num += 10
print("num:", num) 

#logical operators
a=50
b=30
print(not False)
print(not(a>b))

val1=True
val2=False
print("ans operator:",val1 and val2)
