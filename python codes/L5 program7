#range : range(start?,stop,step?) 
     #stop is compulsory..

# eg:- range(3)= 0,1,2 not 3
print(range(5))

seq=range(5)
print(seq[0])
print(seq[1])
print(seq[2])
print(seq[3])
print(seq[4])


seq=range(5) #0,1,2,3,4 only

for i in  seq:
    print(i)

 #OR

for i in range(5): #range(stop) ; 0,1,2,3,4 
    print(i)

for i in range(2, 10): #range(start, stop) ; 2,3,4,5,6,7,8,9 
    print(i)

for i in range(2, 10, 2): #range(start, stop , increases by OR step)
    print(i)