#list in python = '[]'

marks=[94.4,87.5,95.2,66.4,45.1]
print(marks)
print(len(marks))
print(marks[0])
print(marks[1])
print(type(marks))


#NOTE:  string is immutable = ""
# list is mutable = []
# tuple is immutable = ()
# dictionary is mutable = {}
# set is mutable = {} but elements in a set is immutable so, we cannot store list and dict in Set